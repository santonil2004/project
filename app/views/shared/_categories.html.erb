<!--<%= Category.list_categories.inspect %>-->
<ul class="nav nav-list">
<li class="nav-header">Categories</li>
	<% Category.list_categories.each do |category| %>
      <li class=<%= 'active' if category.id.to_s()==params[:id] %>>
      	<%= link_to category.name.capitalize, "/posts/category/"+category.id.to_s() %>
      </li>
    <% end %>
</ul>